#include <stdio.h>
#include <stdlib.h>

 void main ()
{
   
    int valor, ant, suc;
    
    printf("Entre com um valor: ");
    scanf("%d", &valor);
    
    ant = valor -1;
    suc = valor +1;
    printf("O antecessor de %d é %d \n\n", valor, ant);
    printf("O sucessor de %d é %d \n\n", valor, suc);
    system("pause");
    
}



